{
  "expo": {
    "name": "Adipan",
    "slug": "adipan",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/logo4.png",
    "splash": {
      "image": "./assets/splash-adipan.png",
      "resizeMode": "cover",
      "backgroundColor": "#6A1B1A"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "android": {
      "package": "com.adipan.app",
      "googleServicesFile": "./google-services.json",
      "versionCode": 1,
      "permissions": [
        "INTERNET",
        "CAMERA",
        "READ_EXTERNAL_STORAGE",
        "WRITE_EXTERNAL_STORAGE"
      ],
      "adaptiveIcon": {
        "foregroundImage": "./assets/logo4.png",
        "backgroundColor": "#6A1B1A"
      }
    },
    "ios": {
      "bundleIdentifier": "com.adipan.app",
      "buildNumber": "1.0.0"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "runtimeVersion": {
      "policy": "sdkVersion"
    },
    "plugins": [
      "@react-native-firebase/app",
      "@react-native-firebase/auth"
    ],
    "extra": {
      "eas": {
        "projectId": "3db793cb-b01e-4369-b2dc-006072851ec3"
      }
    },
    "owner": "renezc"
  }
}

